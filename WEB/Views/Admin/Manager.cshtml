
@{
    ViewBag.Title = "Manager";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-3">
        <div class="list-group" id="list-tab" role="tablist">
            <a class="list-group-item list-group-item-action active" id="list-home-list" data-toggle="list" href="#statistic" role="tab">Thống kê</a>
            <a class="list-group-item list-group-item-action" id="list-home-list" data-toggle="list" href="#user" role="tab">Người dùng</a>
            <a class="list-group-item list-group-item-action" id="list-home-list" data-toggle="list" href="#category" role="tab">Danh mục</a>
            <a class="list-group-item list-group-item-action" id="list-profile-list" data-toggle="list" href="#publisher" role="tab">Nhà xuất bản</a>
            <a class="list-group-item list-group-item-action" id="list-messages-list" data-toggle="list" href="#author" role="tab">Tác giả</a>
            <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#type" role="tab">Hình thức</a>
            <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#language" role="tab">Ngôn ngữ</a>
        </div>
    </div>
    <div class="col-9">
        <div class="tab-content" id="nav-tabContent">
            <div class="tab-pane fade show active" id="statistic" role="tabpanel">@RenderPage("~/Views/Admin/Shared/Statistic.cshtml")</div>
            <div class="tab-pane fade" id="user" role="tabpanel">@RenderPage("~/Views/Admin/Shared/User.cshtml")</div>
            <div class="tab-pane fade" id="category" role="tabpanel">@RenderPage("~/Views/Admin/Shared/Category.cshtml")</div>
            <div class="tab-pane fade" id="publisher" role="tabpanel">@RenderPage("~/Views/Admin/Shared/Publisher.cshtml")</div>
            <div class="tab-pane fade" id="author" role="tabpanel">@RenderPage("~/Views/Admin/Shared/Author.cshtml")</div>
            <div class="tab-pane fade" id="type" role="tabpanel">@RenderPage("~/Views/Admin/Shared/Type.cshtml")</div>
            <div class="tab-pane fade" id="language" role="tabpanel">@RenderPage("~/Views/Admin/Shared/Language.cshtml")</div>
        </div>
    </div>
</div>